{
    "name": "gregwar/form-bundle",
    "description": "Provides the \"entity_id\" type (read \"entity identifier\")",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "symfony/form": "^5.0 || ^6.0",
        "symfony/validator": "^5.0 || ^6.0",
        "symfony/property-access": "^5.0 || ^6.0",
        "doctrine/orm": "^2.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3 || ^2.0",
        "symfony/twig-bundle": "^5.0 || ^6.0",
        "symfony/twig-bridge": "^5.0 || ^6.0",
        "symfony/yaml": "^5.0 || ^6.0",
        "symfony/templating": "^5.0 || ^6.0",
        "rector/rector": "^0.18.5",
        "friendsofphp/php-cs-fixer": "^3.35.1"
    },
    "autoload": {
        "psr-4": {
            "Gregwar\\FormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gregwar\\FormBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev"
        }
    },
    "scripts": {
        "rector": "rector process",
        "rector:dry": "rector process --dry-run",
        "cs-fix": "php-cs-fixer fix --verbose",
        "cs-fix:dry": "php-cs-fixer fix --dry-run",
        "tests": "phpunit"
    }
}
